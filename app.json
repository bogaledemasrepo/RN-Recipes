{
  "expo": {
    "name": "Recipe",
    "slug": "rn-recipes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bogye.Recipe"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.bogye.Recipe"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#e3e3e3",
          "image": "./assets/icon.png",
          "resizeMode": "cover",
          "imageWidth": 400,
          "enableFullScreenImage_legacy": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "5095eb29-29e7-438a-b971-c667dc3dfad1"
      }
    },
    "owner": "bogitech",
    "updates": {
      "url": "https://u.expo.dev/5095eb29-29e7-438a-b971-c667dc3dfad1",
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD",
      "enableBsdiffPatchSupport": true
    }
  }
}
